            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper
import java.nio.file.*

// Define the CSV file path (relative to the JMeter project directory or absolute path)
def csvFilePath = &apos;user_ids.csv&apos;

// Get the response data from the previous sampler
def responseData = prev.getResponseDataAsString()

// Parse the JSON response
def jsonSlurper = new JsonSlurper()
def parsedJson = jsonSlurper.parseText(responseData)

// Extract the &apos;id&apos; field from the JSON
def userId = parsedJson.id

// Check if the &apos;id&apos; exists
if (userId != null) {
    // Write the &apos;id&apos; to the CSV file
    def csvFile = Paths.get(csvFilePath)
    
    // If the file doesn&apos;t exist, create it and add a header
    if (!Files.exists(csvFile)) {
        Files.write(csvFile, &quot;id\n&quot;.getBytes(), StandardOpenOption.CREATE)
    }
    
    // Append the user ID to the file
    Files.write(csvFile, &quot;${userId}\n&quot;.getBytes(), StandardOpenOption.APPEND)
    
    log.info(&quot;User ID ${userId} saved to ${csvFilePath}&quot;)
} else {
    log.warn(&quot;No &apos;id&apos; found in the response.&quot;)
}
</stringProp>
